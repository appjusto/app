      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "courier-v11-devclient-dev": {
      "extends": "courier-v11-dev",
      "developmentClient": true,
      "ios": {
        "simulator": true
      }
    },
    "courier-v11-store-dev": {
      "extends": "courier-v11-dev",
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      }
