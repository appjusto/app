{
  "build": {
    "courier-v8-internal": {
      "env": {
        "FLAVOR": "courier"
      },
      "releaseChannel": "v8",
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "expoCli": "5.2.0"
    },
    "courier-v8-internal-dev": {
      "extends": "courier-v8-internal",
      "env": {
        "ENV": "dev"
      }
    },
    "courier-v8-internal-staging": {
      "extends": "courier-v8-internal",
      "env": {
        "ENV": "staging"
      }
    },
    "courier-v8-internal-live": {
      "extends": "courier-v8-internal",
      "env": {
        "ENV": "live"
      }
    },
    "courier-v8-live-store": {
      "env": {
        "FLAVOR": "courier",
        "ENV": "live"
      },
      "releaseChannel": "v8",
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      },
      "expoCli": "5.2.0"
    }
  },
  "cli": {
    "version": ">= 0.46.0"
  }
}
