{
  "version": "6.0.0",
  "license": "Apache-2.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start -c",
    "start-dev-client": "expo start -c --dev-client",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint **/*.ts **/*.tsx",
    "prepare-env": "cp .$ENV.env .env",
    "prepare-start": "npm run prepare-env && npm run start",
    "start-consumer": "FLAVOR=consumer npm run prepare-start",
    "start-consumer:dev": "ENV=dev npm run start-consumer",
    "start-consumer:local": "ENV=dev FIREBASE_EMULATOR=true npm run start-consumer",
    "start-consumer:staging": "ENV=staging npm run start-consumer",
    "start-consumer:live": "ENV=live npm run start-consumer",
    "start-courier": "FLAVOR=courier npm run prepare-start",
    "start-courier:dev": "ENV=dev npm run start-courier",
    "start-courier:local": "ENV=dev FIREBASE_EMULATOR=true npm run start-courier",
    "start-courier:staging": "ENV=staging npm run start-courier",
    "start-courier:live": "ENV=live npm run start-courier",
    "inc-version": "node ./version-inc",
    "prepare-publish": "git checkout $ENV && git pull && npm run prepare-env",
    "publish": "expo publish --release-channel $EXPO_RELEASE_CHANNEL",
    "publish-courier:dev": "ENV=dev npm run prepare-env && FLAVOR=courier EXPO_RELEASE_CHANNEL=dev npm run publish",
    "publish-courier:staging": "ENV=staging npm run prepare-env && FLAVOR=courier EXPO_RELEASE_CHANNEL=staging-v5 npm run publish",
    "publish-courier:live": "ENV=live npm run prepare-env && FLAVOR=courier EXPO_RELEASE_CHANNEL=live-v5 npm run publish",
    "publish-consumer:dev": "ENV=dev npm run prepare-env && FLAVOR=consumer EXPO_RELEASE_CHANNEL=dev npm run publish",
    "publish-consumer:staging": "ENV=staging npm run prepare-env && FLAVOR=consumer EXPO_RELEASE_CHANNEL=staging-v5 npm run publish",
    "publish-consumer:live": "ENV=live npm run prepare-env && FLAVOR=consumer EXPO_RELEASE_CHANNEL=live-v5 npm run publish",
    "build": "npm run prepare-env && npm run install-remote-types && EAS_NO_VCS=1 eas build --no-wait --platform $EAS_PLATFORM --profile $EAS_PROFILE && npm run install-local-types",
    "build-android": "EAS_PLATFORM=android EAS_PROFILE=$FLAVOR-$ENV npm run build",
    "build-android-courier:local": "FLAVOR=courier ENV=dev EAS_PROFILE=courier-local npm run build",
    "build-android-courier:dev": "FLAVOR=courier ENV=dev npm run build-android",
    "build-android-courier:staging": "FLAVOR=courier ENV=staging npm run build-android",
    "build-android-courier:live": "FLAVOR=courier ENV=live npm run build-android",
    "build-android-consumer:local": "FLAVOR=consumer ENV=dev EAS_PROFILE=consumer-local npm run build",
    "build-android-consumer:dev": "FLAVOR=consumer ENV=dev npm run build-android",
    "build-android-consumer:staging": "FLAVOR=consumer ENV=staging npm run build-android",
    "build-android-consumer:live": "FLAVOR=consumer ENV=live npm run build-android",
    "build-ios": "EAS_PLATFORM=ios npm run build",
    "build-ios-courier:dev": "FLAVOR=courier ENV=dev npm run build-ios",
    "build-ios-courier:staging": "FLAVOR=courier ENV=staging npm run build-ios",
    "build-ios-courier:live": "FLAVOR=courier ENV=live npm run build-ios",
    "build-ios-consumer:dev": "FLAVOR=consumer ENV=dev npm run build-ios",
    "build-ios-consumer:staging": "FLAVOR=consumer ENV=staging npm run build-ios",
    "build-ios-consumer:live": "FLAVOR=consumer ENV=live npm run build-ios",
    "classic-build": "npm run prepare-env && expo build:$EAS_PLATFORM -t $EXPO_BUILD_TYPE --release-channel $EXPO_RELEASE_CHANNEL",
    "classic-build-android": "EAS_PLATFORM=android EXPO_BUILD_TYPE=${EXPO_BUILD_TYPE:-apk} npm run classic-build",
    "classic-build-android-courier": "FLAVOR=courier EXPO_RELEASE_CHANNEL=${EXPO_RELEASE_CHANNEL:-$ENV} npm run classic-build-android",
    "classic-build-android-courier:dev": "ENV=dev npm run classic-build-android-courier",
    "classic-build-android-courier:staging": "ENV=staging EXPO_RELEASE_CHANNEL=staging-v5 npm run classic-build-android-courier",
    "classic-build-android-courier:live": "ENV=live EXPO_RELEASE_CHANNEL=live-v5 EXPO_BUILD_TYPE=app-bundle npm run classic-build-android-courier",
    "classic-build-android-consumer": "FLAVOR=consumer EXPO_RELEASE_CHANNEL=${EXPO_RELEASE_CHANNEL:-$ENV} npm run classic-build-android",
    "classic-build-android-consumer:dev": "ENV=dev npm run classic-build-android-consumer",
    "classic-build-android-consumer:staging": "ENV=staging EXPO_RELEASE_CHANNEL=staging-v5 npm run classic-build-android-consumer",
    "classic-build-android-consumer:live": "ENV=live EXPO_RELEASE_CHANNEL=live-v5 EXPO_BUILD_TYPE=app-bundle npm run classic-build-android-consumer",
    "classic-build-ios-consumer:staging": "ENV=staging npm run prepare-env && FLAVOR=consumer expo build:ios -t archive --release-channel staging-v5",
    "classic-build-ios-consumer:live": "ENV=live npm run prepare-env && FLAVOR=consumer expo build:ios -t archive --release-channel live-v5",
    "install-local-types": "npm uninstall @appjusto/types && npm install ../types",
    "install-remote-types": "npm uninstall @appjusto/types && npm install @appjusto/types",
    "clear-cache": "watchman watch-del-all && rm -rf node_modules/ && rm -f package-lock.json && npm install --legacy-peer-deps",
    "version": "node ./version-inc && git add version.json",
    "postversion": "git push && git push --tags && git fetch"
  },
  "dependencies": {
    "@appjusto/types": "file:../types",
    "@expo/react-native-action-sheet": "^3.8.0",
    "@expo/vector-icons": "^12.0.0",
    "@fnando/cnpj": "^1.0.2",
    "@fnando/cpf": "^1.0.2",
    "@mapbox/polyline": "^1.1.1",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/slider": "3.0.3",
    "@react-navigation/bottom-tabs": "^5.7.1",
    "@react-navigation/native": "^5.6.1",
    "@react-navigation/stack": "^5.6.2",
    "@types/algoliasearch": "^3.34.11",
    "@types/googlemaps": "^3.43.2",
    "@types/lodash": "^4.14.158",
    "algoliasearch": "^4.8.3",
    "axios": "^0.21.1",
    "dayjs": "^1.8.36",
    "dotenv": "^8.2.0",
    "expo": "^42.0.0",
    "expo-analytics-segment": "~10.2.1",
    "expo-app-loading": "^1.0.1",
    "expo-application": "~3.2.0",
    "expo-asset": "~8.3.2",
    "expo-camera": "~11.2.1",
    "expo-constants": "~11.0.1",
    "expo-dev-client": "^0.4.7",
    "expo-device": "~3.3.0",
    "expo-font": "~9.2.1",
    "expo-image-manipulator": "~9.2.2",
    "expo-image-picker": "~10.2.2",
    "expo-keep-awake": "~9.2.0",
    "expo-linking": "~2.3.1",
    "expo-localization": "~10.2.0",
    "expo-location": "~12.1.2",
    "expo-media-library": "~12.1.2",
    "expo-notifications": "~0.12.2",
    "expo-permissions": "~12.1.1",
    "expo-splash-screen": "~0.11.2",
    "expo-task-manager": "~9.2.2",
    "expo-updates": "~0.8.0",
    "firebase": "^8.8.1",
    "geofirestore": "^4.2.1",
    "geokit": "^1.1.0",
    "i18n-js": "^3.7.1",
    "lodash": "^4.17.19",
    "marked": "^2.0.1",
    "nanoid": "^3.1.10",
    "normalizr": "^3.6.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
    "react-native-maps": "0.28.0",
    "react-native-pager-view": "5.0.12",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.4.0",
    "react-native-svg": "12.1.1",
    "react-native-web": "~0.13.12",
    "react-native-webview": "11.6.2",
    "react-native-youtube-iframe": "^2.1.0",
    "react-query": "3.5.11",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "sentry-expo": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@types/i18n-js": "^3.0.3",
    "@types/mapbox__polyline": "^1.0.0",
    "@types/marked": "^2.0.1",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "@types/react-redux": "^7.1.9",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "babel-preset-expo": "8.3.0",
    "eslint": "^7.5.0",
    "eslint-config-universe": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.0.5",
    "typescript": "~4.0.0"
  },
  "private": true
}
